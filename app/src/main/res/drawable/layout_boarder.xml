<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="10dp"
        android:right="10dp"
        android:top="10dp"
        android:bottom="10dp"
        >
        <shape
            android:shape="rectangle">
            <padding
                android:top="5dp"
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"/>
            <corners
                android:radius="15dp"
                />

            <solid android:color="#fff"
                />
        </shape>

    </item>


</layer-list>